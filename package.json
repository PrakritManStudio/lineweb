{
  "name": "@prakrit_m/lineweb",
  "version": "1.0.0-beta.5",
  "description": "Unofficial TypeScript client for LINE OA Web/Inbox (chat.line.biz)",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./type": {
      "types": "./dist/type/index.d.ts",
      "import": "./dist/type/index.js",
      "require": "./dist/type/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepare": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier -w .",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build && npm run test"
  },
  "engines": {
    "node": ">=18.18"
  },
  "keywords": [
    "line",
    "line oa",
    "line business",
    "inbox",
    "chat.line.biz",
    "sdk",
    "typescript",
    "axios",
    "unofficial"
  ],
  "author": "Prakrit M.",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PrakritManStudio/lineweb.git"
  },
  "bugs": {
    "url": "https://github.com/PrakritManStudio/lineweb/issues"
  },
  "homepage": "https://github.com/PrakritManStudio/lineweb#readme",
  "dependencies": {
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@line/bot-sdk": "^10.2.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
